def calculate_values(nums1, nums2):
    # Calculate answer1
    answer1 = sum(1 for num in nums1 if num in nums2)
    
    # Calculate answer2
    answer2 = sum(1 for num in nums2 if num in nums1)
    
    return [answer1, answer2]

# Example usage
nums1 = [2, 3, 2]
nums2 = [1, 2]

result = calculate_values(nums1, nums2)
print(result)  
